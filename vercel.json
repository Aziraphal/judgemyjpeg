{
  "buildCommand": "prisma generate && npm run build",
  "framework": "nextjs",
  "functions": {
    "src/pages/api/photos/analyze.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/health",
      "schedule": "*/4 * * * *"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?!judgemyjpeg\\.fr).*"
        }
      ],
      "destination": "https://judgemyjpeg.fr/$1",
      "permanent": true
    }
  ]
}