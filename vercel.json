{
  "buildCommand": "prisma generate && npm run build",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?!judgemyjpeg\\.fr).*\\.vercel\\.app"
        }
      ],
      "destination": "https://judgemyjpeg.fr/$1",
      "permanent": true
    }
  ]
}